>
  <rdf:Description rdf:about="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/6ec6bcea-28d2-4b00-90f3-4fe6ce462f67"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/3b28f470-9844-480a-b1e6-4597aec76278"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/d180cbf8-5fdf-4fe1-93d3-fd7bf64e90c0"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/7221dd9c-47da-40b6-9383-4492b664d404"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/5f9a480f-dd24-4bf1-93df-ec782944576c"/>
    <skos:hasTopConcept rdf:resource="http://localhost:8000/293f800d-789c-4d5f-ab61-9906f23ceb3f"/>
    <dcterms:title xml:lang="en">{"id": "91b30273-5e7b-405f-ab92-174ff463c708", "value": "Site Location Certainty"}</dcterms:title>
    <dcterms:identifier xml:lang="en">{"id": "ddc5a8ea-6a2c-4c94-b6d0-101caed9b1c5", "value": "http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/3b28f470-9844-480a-b1e6-4597aec76278">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
    <skos:prefLabel xml:lang="en">{"id": "2438d28d-dab8-480d-a24a-4a6e7dc388cd", "value": "High"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "c3ed2e7b-7dad-47a6-a0ad-c98cb4174d06", "value": "http://localhost:8000/3b28f470-9844-480a-b1e6-4597aec76278"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "7bd5ed20-4a44-49a7-a66c-2ecdfd8a9e61", "value": "The site/feature can be clearly identified from imagery (preferably multiple images). There is no concern over its 'shifting location' and its location may or may not be corrorobated by placename or cartographic data."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/d180cbf8-5fdf-4fe1-93d3-fd7bf64e90c0">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "4a931d24-3e5f-4d22-ae48-df84e1eb2014", "value": "Negligible"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "a996ab17-52d4-40cc-b209-2e7b413c1a7f", "value": "http://localhost:8000/d180cbf8-5fdf-4fe1-93d3-fd7bf64e90c0"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
    <skos:scopeNote xml:lang="en">{"id": "581541e9-ae4c-4592-8243-1a30bb028ec2", "value": "The site/feature has no accurate spatial information associated with it. For example, it is mentioned within the literature as being within a general region, but there is no additional information or imagery to corroborate this."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/5f9a480f-dd24-4bf1-93df-ec782944576c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
    <skos:prefLabel xml:lang="en">{"id": "db63716f-bc89-46bc-8c80-3adf5913e1e5", "value": "Unknown"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "37bb66d0-9c3a-45e2-acc3-1b221afc43f8", "value": "http://localhost:8000/5f9a480f-dd24-4bf1-93df-ec782944576c"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/293f800d-789c-4d5f-ab61-9906f23ceb3f">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "651eed7c-963b-45f7-8584-ee5b2dd1590d", "value": "The site/feature has been identified in the field, either through primary fieldwork and/or published data and can be clearly seen and identified on the imagery OR exact co-ordinates have been provided/published (and you have no reason to doubt their accuracy), even if the site cannot be seen on the imagery"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "89714b3c-c2c5-40f7-b6ee-4f89af82b538", "value": "http://localhost:8000/293f800d-789c-4d5f-ab61-9906f23ceb3f"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "d902eeef-189a-4337-8cfb-67f62185d686", "value": "Definite"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/7221dd9c-47da-40b6-9383-4492b664d404">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "32c78196-9c91-4395-ad9e-e44ce5254ecd", "value": "The site/feature is identifable on imagery, although there may be some concern over its 'shifting location' in different imagery sets. This may be due to poor imagery, rectification issues and/or limited data."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "6429b1b2-e0ba-4a4e-96ba-2165823ba351", "value": "Medium"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "0b0c3b28-ed6e-4029-9930-0d2ac0d18aaf", "value": "http://localhost:8000/7221dd9c-47da-40b6-9383-4492b664d404"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/6ec6bcea-28d2-4b00-90f3-4fe6ce462f67">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "34433ed9-9796-4d93-a856-f55371a9ea29", "value": "The site/feature is possibly identifiable on imagery or you have an approximate location (e.g. to within less than a kilometre) from sketch maps or descriptions, however, there is concern over the precision of the location due to poor imagery, rectification issues and/or limited data."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "6a26fcbd-8038-4fa3-954d-9df1a0232413", "value": "Low"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/dbd81ce2-dd0e-4de4-b727-1eeb7ba9534e"/>
    <dcterms:identifier xml:lang="en">{"id": "3868ab1c-44c5-4735-9233-80e7bf078d9b", "value": "http://localhost:8000/6ec6bcea-28d2-4b00-90f3-4fe6ce462f67"}</dcterms:identifier>
  </rdf:Description>
</rdf:RDF>
