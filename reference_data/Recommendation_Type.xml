    <skos:prefLabel xml:lang="en">{"id": "730da882-8f77-432f-bbc7-ede4fbbf7d4e", "value": "Construction Monitoring and Heritage Preservation"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "619eb9b8-4974-4cd5-a85c-bf1b7f4b82e1", "value": "The ongoing observation, assessment, and protection of heritage sites during construction or development projects to minimize potential damage."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/c640c0ac-cec0-4b51-b65f-435f1d4885f8">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:scopeNote xml:lang="en">{"id": "6491f804-6df7-46e5-a7b3-12991de34e78", "value": "Efforts to emphasize and showcase the importance of historical buildings and structures in their original or restored state."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5ccbc757-0350-48a5-bd89-5525e99af326", "value": "Highlighting Historical Structures"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/0b08ef61-4690-45dc-8bc2-8f1518925c2c">
    <skos:prefLabel xml:lang="en">{"id": "3d56bcf0-bcce-44c1-9506-3e6b0c6ecba0", "value": "Not Defined"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/f93516fd-f6ae-46ed-9f0a-c5be54a7310c">
    <skos:scopeNote xml:lang="en">{"id": "cfc1d787-6950-4a03-8228-ac78322c6a32", "value": "The combined impact of natural environmental factors and human activities on archaeological sites and cultural heritage."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "9c8eb0ff-078f-4001-8056-5c506fb6f8d9", "value": "Environmental and Anthropogenic Actions"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/cec85f26-d10a-4eee-8651-90b7ff91258b">
    <skos:scopeNote xml:lang="en">{"id": "dbac9f2c-c710-4a3f-a2fb-1dd870873b9e", "value": "Initiatives designed to inform and educate local communities and the public about cultural heritage, archaeological sites, and the importance of preserving them. These activities promote community involvement and foster a sense of responsibility towards heritage conservation."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "c9dbbde3-b291-4073-a051-f532a109c8da", "value": "Community Awareness and Education"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/cdfeeaea-5ce7-4e52-9750-9dfd0ed6d635">
    <skos:prefLabel xml:lang="en">{"id": "2b469a18-08c3-4721-9c2f-4aa253734ace", "value": "Conservation and Preservation"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "5bb5ec48-20f4-417f-901d-e8c30b4a2354", "value": "The practice of safeguarding and maintaining archaeological sites, artifacts, and cultural heritage to prevent deterioration or loss."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/a591c7f1-8a45-463a-9683-e507a7fedf9f">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:scopeNote xml:lang="en">{"id": "a489af15-c585-4a1d-a4f9-eeb249372cec", "value": "Initiatives and programs that promote education about cultural heritage, emphasizing its significance and the need for preservation."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "2fb9338a-5263-43f4-b158-6e27415db4d6", "value": "Heritage Education and Conservation"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/9b4954d7-0489-4439-92ff-9efd13c345ee">
    <skos:scopeNote xml:lang="en">{"id": "effabce7-806f-4e44-903d-e5e720d896ec", "value": "The study, interpretation, and cultural utilization of archaeological findings and heritage resources."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "3a9b741c-bdf0-46ca-a35f-182577f2854e", "value": "Research and Cultural Utilization"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/bab63e3d-d7eb-46a5-908f-b97e237fecb2">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:prefLabel xml:lang="en">{"id": "8a00d3ed-8b4e-4c32-915d-947ee09524e0", "value": "Municipal Planning and Regulations"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "8701b912-008e-44b7-a1c7-a5e5b2a7f90f", "value": "The development of local government policies, laws, and regulations aimed at managing, protecting, and conserving heritage sites and historical structures within a municipality or region."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/7894ad6b-271e-4b06-a9ce-009e63c3af41">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:scopeNote xml:lang="en">{"id": "6ea47d97-5878-4f0d-8dee-93dd8b36b2b8", "value": "Activities and campaigns designed to assess the condition and significance of archaeological and heritage resources, as well as initiatives aimed at increasing public awareness and support for their protection and conservation."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "d489b1a0-e401-422e-a93c-b4f4e9eb0f5a", "value": "Assessment and Awareness Campaigns"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/9d7e69e3-563b-4405-996b-bd222fd9bf04">
    <skos:scopeNote xml:lang="en">{"id": "f1d7040b-53bd-4616-8215-e16a6ccbc750", "value": "The formal process of notifying relevant legal authorities, property owners, and stakeholders about archaeological findings, site preservation requirements, or any actions required to comply with heritage preservation laws and regulations."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:prefLabel xml:lang="en">{"id": "8067cbca-c37b-47b5-815f-0db49c42606c", "value": "Legal and Administrative Notifications"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/a938d9aa-0645-499e-88a6-21162f6c50c8">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:prefLabel xml:lang="en">{"id": "19f09d49-99c0-4bd0-b9f7-2e5b2769f23f", "value": "Archaeological Research and Conservation"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "0459738f-b414-4705-ae76-27e3f597c6ce", "value": "The systematic study, exploration, and preservation of archaeological sites, artifacts, and cultural heritage."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/59f76b85-208e-4155-8ad8-3462525cdee5">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:prefLabel xml:lang="en">{"id": "91aed053-4ecd-43fb-9dad-5c92d6ca21f3", "value": "Potential Archaeological Rescue"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "e1af9053-b3c0-4344-b5c6-b574d0c8fa42", "value": "The pre-emptive planning and action taken to rescue or salvage archaeological sites and cultural heritage when they are at risk of damage or destruction due to construction or development projects."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/4895348e-0db6-4d04-8168-75000e2cf10c">
    <skos:prefLabel xml:lang="en">{"id": "bdc27115-c831-4fc5-8ff7-5451059c20aa", "value": "Other"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/70ae9aba-3170-4cf3-9f95-0b678ef4c1ff">
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
    <skos:prefLabel xml:lang="en">{"id": "0da9bca5-a3f1-4293-8066-5a6d224c6de3", "value": "Protective Measures"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "f6e2bd5c-e627-497a-b219-4adb1cd1592f", "value": "Strategies and actions put in place to protect and safeguard archaeological sites, artifacts, and cultural heritage from potential harm, including vandalism, natural disasters, or development activities."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/f82de7ec-c3b6-45ac-99c0-e6618b3e3fba">
    <skos:scopeNote xml:lang="en">{"id": "eceb9593-bb81-4cbf-bc57-185eb5da631a", "value": "The assessment and management of the potential environmental impact of construction, development, or land use on archaeological and heritage sites."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "bba753ed-0c44-4c0e-b96f-cc403fbb9374", "value": "Environmental Impact and Salvage"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/958ec0be-4734-49ef-9b76-c89928daf4ea"/>
  </skos:Concept>
