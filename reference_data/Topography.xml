    <skos:inScheme rdf:resource="http://localhost:8000/920e27b5-221b-478b-ad02-af838426a1d0"/>
    <skos:prefLabel xml:lang="en">{"id": "63788763-cb05-4af1-93fd-7193dd8db53c", "value": "Summit"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "22615f50-e93f-4a80-9840-7612f5b6a61d", "value": "The highest point/points of an area of upland terrain (e.g. mountains, hills etc.). This cannot be used to refer to the entire hill or mountain and solely refers to the top of one of these features."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "b04b680e-6260-49c8-8219-1b2f0b01dede", "value": "http://localhost:8000/bf0d84d2-68a4-4862-9642-a0a768de8d0c"}</dcterms:identifier>
  </rdf:Description>
