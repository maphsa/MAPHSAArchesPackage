  <rdf:Description rdf:about="http://localhost:8000/a02a6115-c088-4349-8e84-a15d4a01d9a2">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "2461ccc9-ca1a-484d-81bd-a62aa0aebf50", "value": "Intertidal Flat"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "dbca7f86-95d8-415c-824b-c6b9c88d5fa7", "value": "Extensive flat or gently sloping area of unconsolidated fine sediment (generally mud to sand) covered by water at high tide and exposed at low tide"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "53d198ed-47bc-4bb0-9c0a-cb949feee894", "value": "http://localhost:8000/a02a6115-c088-4349-8e84-a15d4a01d9a2"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/920e27b5-221b-478b-ad02-af838426a1d0"/>
