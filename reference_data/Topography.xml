  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/3a95343b-676c-4aee-8f6a-193d387fb932">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "9bc10425-9c69-4bec-b373-309a11047b77", "value": "The area of land adjacent/alongside an inland body of water. The edge of this is usually defined by the presence of another topographical feature e.g. slopes"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "a677b8fe-21c1-45cb-9a05-96c1ce7fad46", "value": "Lake Shore"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "9cfd3d6c-3547-4248-8623-f699fb3871f4", "value": "http://localhost:8000/3a95343b-676c-4aee-8f6a-193d387fb932"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/920e27b5-221b-478b-ad02-af838426a1d0"/>
  </rdf:Description>
