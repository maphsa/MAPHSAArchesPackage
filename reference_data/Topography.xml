  <rdf:Description rdf:about="http://localhost:8000/de6220da-c790-454b-90bd-250e4f3a32db">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "0b0094e9-4f54-4994-a915-56295c61e91d", "value": "Vertical to near-vertical faces of exposed rock or sediment located at the shoreline and produced by wave erosion"}</skos:scopeNote>
