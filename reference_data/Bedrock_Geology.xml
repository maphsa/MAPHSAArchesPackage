  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/09b918f5-260f-4e3b-97d4-8740c3d33c9a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "d1aec990-848e-4cf6-86a1-6e019db7688b", "value": "Tuff"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "505850f4-078a-4723-8610-38ac632cec77", "value": "http://localhost:8000/09b918f5-260f-4e3b-97d4-8740c3d33c9a"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "80206c79-f90d-46f0-89db-d124d8bddd4c", "value": "Rock produced by the consolidation of accumulated volcanic material including ash, tephra and lava and often incorporating other rocks or material pre-existing at the point of deposition"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/d2a0dcf5-bfe0-44d5-9c43-47b9d6ecc50e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "7a7ce04e-5549-49c1-ba17-22c30f04cc90", "value": "Sedimentary rock produced by the consolidation of organic material accumulated from organic debris such as plant matter. Common types include coal and lignite"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
    <dcterms:identifier xml:lang="en">{"id": "de81eb5b-6d5b-420e-b6e8-d801d760eaca", "value": "http://localhost:8000/d2a0dcf5-bfe0-44d5-9c43-47b9d6ecc50e"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "3ee8562c-0365-4c73-ac94-2f98a60cf0aa", "value": "Organic sedimentary rock"}</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/04b19982-9fd5-4a00-818f-a503fe1ef70d">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "e43af628-cd7e-473b-a152-a5310b24324a", "value": "Sedimentary (undefined)"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "ad65c6d9-a347-4ea1-884f-367dc18af537", "value": "Rock formed by the accumulation and consolidation of loose sediment orginally deposited by any one, or several, of a range of transport / formation processes. For example including wind action, water action, glacial action, precipiation and biogenic processes. Use this term where the rock is sedimentary but the specific type (e.g. sandstone, shale) is unknown"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "7999e640-7255-4bc4-bbd8-ea4f67392c08", "value": "http://localhost:8000/04b19982-9fd5-4a00-818f-a503fe1ef70d"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/c2510b6e-7a60-40c6-9a1a-bf515ab322c8">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "fdc144bf-08d2-4455-8ddb-d55a6b84874a", "value": "http://localhost:8000/c2510b6e-7a60-40c6-9a1a-bf515ab322c8"}</dcterms:identifier>
