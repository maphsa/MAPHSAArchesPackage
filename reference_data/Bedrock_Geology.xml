    <skos:scopeNote xml:lang="en">{"id": "0e03b81b-c52e-4645-a12c-7b9c9c8b8bfe", "value": "Fine- to medium grained  sedimentary rock produced by the accumulation and consolidation of sand-sized clasts"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "2d63848a-fd01-4c75-8606-20cab9358c8e", "value": "Sandstone"}</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/4b7dfdd1-0228-48d2-92c3-8bbbf74a1f78">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "24244d4e-1bc9-401d-8ab5-cabdf615416b", "value": "Sedimentary rock containing large gravel sized clasts (&gt;2mm) usually embedded within a matrix of finer grains"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
    <skos:prefLabel xml:lang="en">{"id": "64123b76-0f45-477f-8d35-1e228b8843c2", "value": "Conglomerate"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "86dc210a-0a62-46a6-95d0-304235668a2e", "value": "http://localhost:8000/4b7dfdd1-0228-48d2-92c3-8bbbf74a1f78"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/a676635b-f532-42ec-9976-180b592f024e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "2b445b9e-0d34-43bf-8ec4-2d2cb64992ad", "value": "http://localhost:8000/a676635b-f532-42ec-9976-180b592f024e"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "37708c74-1355-42e1-81a8-09ebbdaa14f7", "value": "Dark, fine-grained igeneous rock, generally formed by rapid cooling of lava"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
    <skos:prefLabel xml:lang="en">{"id": "969b25b3-c2f7-441a-a7c5-5fd60ef6ccd7", "value": "Basalt"}</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/61e8a964-3877-4236-9b09-e75e85b85dc3">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "0acdf2a0-93d0-4c77-8cef-937b53334209", "value": "Rock formed by cooling and solidification of molten rock including within the Earth as well as from eruptions/extrusion of lava. Use this term where the rock is igneous, but the specific type (e.g. basal, granite) is unknown"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "8f041467-6546-4e50-b6ce-93ac6b674b11", "value": "Igenous (undefined)"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "aea76526-4098-42c8-951d-41bfe870abf8", "value": "http://localhost:8000/61e8a964-3877-4236-9b09-e75e85b85dc3"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/6b6cf6e2-c861-440e-9f1b-4af03335b102">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
    <dcterms:identifier xml:lang="en">{"id": "665d2f8e-79f3-4dac-bd57-ddc30dd2a702", "value": "http://localhost:8000/6b6cf6e2-c861-440e-9f1b-4af03335b102"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "10133e20-b0b1-4a0f-8e83-d1263e6a08cb", "value": "Granite"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "4c49ee44-db14-4458-bff0-944a1944bad4", "value": "Coarse-to medium grained igeneous rock, generally formed by slow cooling of molten rock within the Earth's surface "}</skos:scopeNote>
  </rdf:Description>
