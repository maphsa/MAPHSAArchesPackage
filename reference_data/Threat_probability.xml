  <rdf:Description rdf:about="http://localhost:8000/e5d3d964-24f0-4394-abfa-a0aca28b1b3e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/f8776d0f-53fe-44c0-a445-140c2db30e96"/>
    <skos:prefLabel xml:lang="en">{"id": "3a557f20-f17d-40ea-a12a-e93e75b775ce", "value": "Certain/Planned"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "92af3909-ada2-44ea-bd8b-c3cd3758e95f", "value": "http://localhost:8000/e5d3d964-24f0-4394-abfa-a0aca28b1b3e"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "a966fa19-ea69-4d97-97d7-b91a22752eb9", "value": "It is certain that the identified threat will occur and harm the heritage; e.g. a planned road crossing through the site"}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/9fc51713-48df-403a-bebf-9da814d8f02e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "70067ba4-ac8a-4d96-9cb5-05685d9ae9fd", "value": "http://localhost:8000/9fc51713-48df-403a-bebf-9da814d8f02e"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "0dbd610c-d4e6-49be-a9f3-7e981c5cd7d7", "value": "Very likely"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "921cbe5c-2aaf-4f29-9e39-944cf7ca9c57", "value": "The investigator has considerable reason (e.g. proximity to archaeology, nature/source of threat etc.,) to suggest that the threat is likely to cause future disturbance to the site (within 3-5 years). For example, it would appear to be an ongoing process e.g. urban expansion, ploughing etc."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/f8776d0f-53fe-44c0-a445-140c2db30e96"/>
  </rdf:Description>
