>
  <rdf:Description rdf:about="http://localhost:8000/662429ae-9e18-4103-9ca6-7948ec50d17b">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:prefLabel xml:lang="en">{"id": "2c6c7887-5a6f-490a-9477-24de8e7f0c2f", "value": "Water Damage"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "d40f670b-70e3-4cc1-8d28-58370f448ba0", "value": "http://localhost:8000/662429ae-9e18-4103-9ca6-7948ec50d17b"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "6112c539-e247-40a6-9bc6-2669eb93340e", "value": "The effects of water on archaeological features and sites, this includes rising damp, structural decay, rotting, rusting etc."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/d57e72c6-5228-43ad-91dd-5135601e9c51">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "a079a556-1c41-461c-b3d5-d0369e97c216", "value": "http://localhost:8000/d57e72c6-5228-43ad-91dd-5135601e9c51"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "0f2d0616-b9a5-475d-a5a9-e65533cdf68f", "value": "Not Applicable"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:scopeNote xml:lang="en">{"id": "89a40af2-b08c-4682-ad10-16edc16054f2", "value": "Only to use when the cause is \"No Visible/Known\"."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/7761441b-7917-4fea-ab31-de9e5203da7a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "cf3a88f1-23b4-4abd-b1d2-fb7831dce469", "value": "The damage caused to archaeological sites/features via fire. This can include both damage caused by the flames of the fire, as well as smoke damage."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "938cc2fb-e8c3-4ee5-b7fc-6041122e26e6", "value": "Burning"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <dcterms:identifier xml:lang="en">{"id": "f2b23f31-652f-4a9c-9325-f726724ae4ad", "value": "http://localhost:8000/7761441b-7917-4fea-ab31-de9e5203da7a"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/790e0d53-5dc3-4083-9e3e-968ca5506f37">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "f21163f1-4ebc-4c21-a0dc-1e28f6874f6a", "value": "http://localhost:8000/790e0d53-5dc3-4083-9e3e-968ca5506f37"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "e5285493-3761-4fbd-bfc9-a880ad5a3290", "value": "Damage or alteration to an archaeological site/feature where the result does not cause the feature to become structurally unsound/compromised."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5edad0f1-7573-4b3a-aa92-70fedb1f30e8", "value": "Structural Alteration"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/4abb9307-ba85-40e4-b7c3-d8c3ed514f24">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <dcterms:identifier xml:lang="en">{"id": "b1651b95-ef77-44a4-b25a-405b7d039d00", "value": "http://localhost:8000/4abb9307-ba85-40e4-b7c3-d8c3ed514f24"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "0c4422e4-2557-4620-b9c4-da28c65ba277", "value": "Covering/Burying/Concealing"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "40df332a-28f0-45fe-9190-46103729eabc", "value": "Deposits burying/concealing features of archaeological significance, this can include the effects of sedimentation, rock falls, dumping of non-local material and/or the erection of new buildings or structures. It also covers sites that are only partially covered."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/85a2f5d8-73b1-4c86-9a32-04042da6ab1e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="en">{"id": "ab485f69-4f11-48d7-9f94-8d06933c9966", "value": "The dispersal/spread of artefacts/objects across the surface or subsurface of an archaeological site. This is often the result of ploughing."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5a062f43-a35e-4ad1-885a-32e22b0b68f6", "value": "Artefact or Feature Displacement"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "3d812c5a-bfc7-4e09-b752-7ef0da9aaa59", "value": "http://localhost:8000/85a2f5d8-73b1-4c86-9a32-04042da6ab1e"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/f5b3ce53-0839-4a36-9092-bbf0c41612b0">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "76c1b1a8-0c4b-4339-b4d4-3152ee279db2", "value": "http://localhost:8000/f5b3ce53-0839-4a36-9092-bbf0c41612b0"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:prefLabel xml:lang="en">{"id": "43371da0-9e4e-449e-bdc5-11ecb5aed899", "value": "Vegetation Covering"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "644d3867-a372-4b7b-9eb3-a87b5265aaf3", "value": "Vegetation or trees burying/concealing features of archaeological significance."}</skos:scopeNote>
  </rdf:Description>
