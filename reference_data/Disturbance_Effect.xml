    <dcterms:identifier xml:lang="en">{"id": "498048f7-4d14-4634-a82f-c2b6ab119e59", "value": "http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/729ebb4d-92a3-4583-91af-2ed8603846b7">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <dcterms:identifier xml:lang="en">{"id": "6414e989-a073-4b88-a8bd-67001dd17157", "value": "http://localhost:8000/729ebb4d-92a3-4583-91af-2ed8603846b7"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "fafb482c-f2c7-4e68-b960-8ac64479748e", "value": "Erosion/Deterioration"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "ee5c0d6f-e646-476e-b2e3-1957eb948ab2", "value": "The process of archaeological material being worn away/weathered, often due to natural causes. (e.g. exposure to wind, rain, sunlight, as well as salt crystalization, insects and pests damage, and growth of micro-organisms)"}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/4e9f5f7c-096a-4442-b6b1-ce9d860fc7dc">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:identifier xml:lang="en">{"id": "796818ec-18ba-4bac-90b2-10192a8ef711", "value": "http://localhost:8000/4e9f5f7c-096a-4442-b6b1-ce9d860fc7dc"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:scopeNote xml:lang="en">{"id": "acb50b21-1ef9-42d1-beda-31a123ef03d3", "value": "Loss of the upper layer of the feature."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "35979d2a-032a-4c28-9a4d-7e2f79deebca", "value": "Surface Loss"}</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/df6d92dd-39dc-44a3-b0e7-31e37c4ff595">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "92ca1b01-dca9-498d-8279-fd49515e63c3", "value": "Not Visible/Known"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "bf54bc12-fb10-4164-9ebd-5d0571c257e8", "value": "http://localhost:8000/df6d92dd-39dc-44a3-b0e7-31e37c4ff595"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "21d3c342-25b9-486f-a881-122fdbd52b1b", "value": "This term should be used to indicate that, given the current evidence, there is no evidence for damage and/or threat to the archaeological site mentioned in published imagery or identifiable from imagery sources."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/f5d6dea1-3fbf-45f0-b9ca-6b63c5e1c74d">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "fc313734-a289-4cd4-a400-c0cfca8e8170", "value": "Collapse/Structural Damage"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <dcterms:identifier xml:lang="en">{"id": "c9df239e-e0a3-4f9d-b0ba-3a9ce16fd095", "value": "http://localhost:8000/f5d6dea1-3fbf-45f0-b9ca-6b63c5e1c74d"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "6f481dc7-b151-4709-b5c8-7b1fbff666ad", "value": "Damage causing archaeological sites or features to fall in/subside or to become structurally unsound, or the structural integrity of the site or feature to be compromised. This category includes partial structural deformation, inclination and swelling."}</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/d19c5a3d-795a-4e82-bffb-85fd3c7bc3fc">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:scopeNote xml:lang="en">{"id": "9dd9749f-f4a6-42f7-8a4f-3e94bb644f58", "value": "Changes in the shape/form of landforms or the topography of a site as the result of activities, such as the bulldozing/dumping of non-site/local material."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5f8c8866-bdf6-4ebb-aad0-6f2b28ab5907", "value": "Alteration of Terrain"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "ea49cb5b-2652-46b0-9999-79c1ab52353a", "value": "http://localhost:8000/d19c5a3d-795a-4e82-bffb-85fd3c7bc3fc"}</dcterms:identifier>
  </rdf:Description>
  <rdf:Description rdf:about="http://localhost:8000/7203827c-2ef0-4ab6-8582-7fa856f237d6">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">{"id": "8acddbf1-1721-4cad-94fe-b53d48a467fe", "value": "Compressed/Flattened"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/2a02260c-995b-4050-ae41-6feb40b332b2"/>
    <skos:scopeNote xml:lang="en">{"id": "ac6ccaae-2bd9-491c-8557-30030d18ec90", "value": "The pressing or packing down of archaeological sites/artefacts/soils and so the material becomes flattened and/or compressed."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "d94f80c5-723d-4ae6-96eb-26b994fb76a8", "value": "http://localhost:8000/7203827c-2ef0-4ab6-8582-7fa856f237d6"}</dcterms:identifier>
  </rdf:Description>
</rdf:RDF>
